@model List<Category>

<nav class="navbar navbar-dark bg-primary navbar-expand-lg">
  <div class="container">
    <button
      type="button"
      class="navbar-toggler"
      data-bs-toggle="collapse"
      data-bs-target="#navbar"
    >
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbar">
      <ul class="navbar-nav">
      
        <li class="nav-item">
          <a href="#" class="nav-link ps-0 active">Anasayfa</a>
        </li>

        
          @foreach(var item in Model){
          if (item.IsActive == false)
          continue;
          else{
              <li class="nav-item">
                  <a href="/products/@item.Url" class="nav-link ps-0">@item.CategoryName</a>
              </li>}
          }
        
        </li>
      </ul>
    </div>
  </div>
</nav>
